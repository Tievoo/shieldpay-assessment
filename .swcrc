{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true
        },
        "transform": {
            "decoratorMetadata": true
        },
        "target": "es2019",
        "keepClassNames": true,
        "loose": true
    },
    "module": {
        "type": "commonjs"
    },
    "minify": false,
    "sourceMaps": true
}
